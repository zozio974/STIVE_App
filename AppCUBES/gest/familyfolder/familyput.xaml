<Window x:Class="AppCUBES.familyput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppCUBES"
        mc:Ignorable="d"
        Title="familyput" Height="179" Width="368">
    <Grid>
        <TextBox x:Name="nameputfamily" HorizontalAlignment="Left" Height="51" Margin="139,20,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="174"/>
        <Button x:Name="validputfamily" Content="Modifier" HorizontalAlignment="Left" Height="45" Margin="156,81,0,0" VerticalAlignment="Top" Width="157" Click="refreshputfamily_Click_1" />
        <TextBlock HorizontalAlignment="Left" Height="25" Margin="30,33,0,0" TextWrapping="Wrap" Text="Entrez un nom" VerticalAlignment="Top" Width="104" RenderTransformOrigin="0.464,0.64"/>
        <Button x:Name="returnfamily2" Content="Retour" HorizontalAlignment="Left" Height="45" Margin="10,81,0,0" VerticalAlignment="Top" Width="141" Click="returnfamily2_Click_1" />
        <TextBox x:Name="invputfamily" HorizontalAlignment="Left" Margin="182,308,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Visibility="Hidden"/>

    </Grid>
</Window>
