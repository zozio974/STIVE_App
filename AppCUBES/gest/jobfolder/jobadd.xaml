<Window x:Class="AppCUBES.jobadd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppCUBES"
        mc:Ignorable="d"
        Title="jobadd" Height="222" Width="276">
    <Grid>
        <TextBox x:Name="namejob" HorizontalAlignment="Left" Height="51" Margin="114,17,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="117"/>
        <Button x:Name="validatejob" Content="valider" HorizontalAlignment="Left" Height="45" Margin="139,80,0,0" VerticalAlignment="Top" Width="92" Click="valjob_Click" />
        <TextBlock HorizontalAlignment="Left" Height="25" Margin="24,30,0,0" TextWrapping="Wrap" Text="Entrez un nom" VerticalAlignment="Top" Width="90" RenderTransformOrigin="0.464,0.64"/>
        <TextBlock x:Name="resjobadd" HorizontalAlignment="Left" Height="34" Margin="24,141,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="207"/>
        <Button x:Name="returnjob" Content="Retour" HorizontalAlignment="Left" Height="45" Margin="22,80,0,0" VerticalAlignment="Top" Width="92" Click="returnjob_Click" />

    </Grid>
</Window>
