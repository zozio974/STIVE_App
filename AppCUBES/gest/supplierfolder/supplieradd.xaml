<Window x:Class="AppCUBES.supplieradd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppCUBES"
        mc:Ignorable="d"
        Title="supplieradd" Height="203" Width="301">
    <Grid>
        <TextBox x:Name="namesupplier" HorizontalAlignment="Left" Height="51" Margin="121,16,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="133"/>
        <Button x:Name="validatesupplier" Content="valider" HorizontalAlignment="Left" Height="45" Margin="162,77,0,0" VerticalAlignment="Top" Width="92" Click="valsupplier_Click" />
        <TextBlock HorizontalAlignment="Left" Height="25" Margin="25,29,0,0" TextWrapping="Wrap" Text="Entrez un nom" VerticalAlignment="Top" Width="91" RenderTransformOrigin="0.464,0.64"/>
        <TextBlock x:Name="ressupplieradd" HorizontalAlignment="Left" Height="34" Margin="25,127,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="229"/>
        <Button x:Name="returnsupplier" Content="Retour" HorizontalAlignment="Left" Height="45" Margin="24,77,0,0" VerticalAlignment="Top" Width="92" Click="returnsupplier_Click" />

    </Grid>
</Window>
